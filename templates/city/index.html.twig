{% block body %}


        <div class="form-group">
            <label for="searchPostCode"></label>
            <input type="text" name="searchPostCode" placeholder="Recherchez une commune ..." id="searchPostCode" minlength="5" maxlength="5">
            <div style="display: none" id="error-message"></div>
        </div>

        <div class="form-group">
            <label for="postcode">City</label>
            <select name="postcode" id="postcode" class="form-control"></select>
        </div>

        <div class="form-group">
            <label for="searchStreet"></label>
            <input type="text" name="searchStreet" placeholder="Recherchez une commune ..." id="searchStreet">
            <div style="display: none" id="error-message"></div>
        </div>

        <div class="form-group">
            <label for="street">Street</label>
            <select name="street" id="street" class="form-control"></select>
        </div>

{% endblock %}
